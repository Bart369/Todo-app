<% include ./partials/boilerplate.ejs %>

<div>
  <% let i = 0; %>
  <div class = 'todoItems'>
  <% todo.forEach((data) => { %>
    <% i++; %>
    <div class = 'todoContainer'>
      <h3><%= data.name %></h3>
      <div class = 'todo<%= i %>'> </div>
      <h3><%= data.todo %></h3>
      <h3>Current Status: <%= data.status %></h3>
    </div>
  <% }) %>
  </div>
</div>

<% include ./partials/end.ejs %>
